<!-- user login in page to log into account -->
  <body>
   
      <div class="col-md-6 login">
        <h2>Login</h2>
    
        <form class="form login-form ">
          <div class="form-group">
            <label for="email-login" id="labelColor">email:</label>
            <input class="form-input forms" type="text" id="email-login" />
          </div>
          <div class="form-group">
            <label for="password-login" id="passwordColor">password:</label>
            <input class="form-input forms" type="password" id="password-login" />
          </div>
          <div class="form-group">
            <button class="btn btn-primary" type="submit">login</button>
          </div>
        </form>
      </div>
    
      <script src="./js/login.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </body>
